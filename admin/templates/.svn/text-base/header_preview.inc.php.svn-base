	<script type="text/javascript">
	function buildMap(address, container){
		var map = new GMap2( document.getElementById( container ) );		
		var geocoder = new GClientGeocoder();	  
		  geocoder.getLatLng(
			address,
			function(point) {
				if (!point) {			
					$("#" + container).html('<b>Localização não encontrada!</b><br />Clique <a href="http://maps.google.com.br/?q=' + address + '" target="_blank">aqui</a> para tentar achar o endereço no Google Maps.');
				}
				else{
					map.setCenter( point, 15 );
					map.setUIToDefault();
					var marker = new GMarker( point );
					map.addOverlay( marker );
				}
			}
		  );
	}	
    </script>
